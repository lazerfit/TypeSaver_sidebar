{
  "manifest_version": 3,
  "name": "TypeSaver",
  "version": "1.1.1",
  "description": "__MSG_appDescription__",
  "default_locale": "ko",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "sidebar_action": {
    "default_page": "index.html",
    "default_icon": {
      "16": "icon16.png"
    },

    "default_title": "TypeSaver",
    "use_navigation_bar": false
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": [
    "storage", "contextMenus", "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}